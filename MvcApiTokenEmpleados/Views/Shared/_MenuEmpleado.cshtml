@if (Context.User.Identity.IsAuthenticated)
{
    <a class="nav-link text-dark" asp-controller="Empleados" asp-action="Index">
        EMPLEADOS
    </a>

    <a class="nav-link" asp-controller="Managed" asp-action="Logout">
        Log Out
        <span style="color:blue">@Context.User.Identity.Name</span>
    </a>
  
}
else
{
    <a class="nav-link text-danger" asp-controller="Empleados" asp-action="Index">
        Log In
    </a>
}